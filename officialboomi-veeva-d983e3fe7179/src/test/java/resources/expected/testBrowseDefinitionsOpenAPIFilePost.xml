<ObjectDefinitions>
    <operationField id="cXVlcnlfa2luZA" label="Query kind" type="string" overrideable="true">
        <helpText>The kind of item to create. This can be either file or folder.</helpText>
        <allowedValue label="">
            <value></value>
        </allowedValue>
        <allowedValue label="file">
            <value>file</value>
        </allowedValue>
        <allowedValue label="folder">
            <value>folder</value>
        </allowedValue>
    </operationField>
    <operationField id="cXVlcnlfcGF0aA" label="Query path" type="string" overrideable="true">
        <helpText>The absolute path, including file or folder name, to place the item in the file staging server. This
            path is specific to the authenticated user. Admin users can access the root directory. All other users can
            only access their own user directory.
        </helpText>
    </operationField>
    <operationField id="cXVlcnlfb3ZlcndyaXRl" label="Query overwrite" type="boolean" overrideable="true">
        <helpText>If set to true, Vault will overwrite any existing files with the same name at the specified
            destination. For folders, this is always false.
        </helpText>
    </operationField>
    <operationField id="cXVlcnlfc2l6ZQ" label="Query size" type="integer" overrideable="true">
        <helpText>TODO size needed but not per documentation!</helpText>
    </operationField>
