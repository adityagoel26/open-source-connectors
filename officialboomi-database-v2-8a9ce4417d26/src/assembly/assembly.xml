<?xml version="1.0" encoding="UTF-8"?>
<assembly>
	<id>car</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<fileSets>
		<fileSet>
			<directory>src/main/resources/META-INF</directory>
			<outputDirectory>META-INF</outputDirectory>
			<includes>
				<include>connector-config.xml</include>
				<include>connector-descriptor.xml</include>
				<include>com.boomi.Overrides</include>
			</includes>
		</fileSet>
	</fileSets>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/lib</outputDirectory>
			<excludes>
				<exclude>com.boomi.connsdk:connector-sdk-api</exclude>
				<exclude>com.boomi.commonsdk:common-sdk</exclude>
				<exclude>commons-codec:commons-codec</exclude>
				<exclude>com.boomi.connsdk:connector-sdk-test-util</exclude>
				<exclude>com.boomi.util:boomi-util</exclude>
				<exclude>com.sun.xml.fastinfoset:FastInfoset</exclude>
				<exclude>com.sun.xml.bind:jaxb-impl</exclude>
				<exclude>com.sun.xml.bind:jaxb-xjc</exclude>
				<exclude>javax.xml.bind:jsr173_api</exclude>
				<exclude>com.oracle.database.jdbc:ojdbc8</exclude>
				<exclude>org.postgresql:postgresql</exclude>
				<exclude>com.microsoft.sqlserve:mssql-jdbc</exclude>
				<exclude>org.slf4j:slf4j-api</exclude>
				<exclude>junit:junit</exclude>
				<exclude>org.hamcrest:hamcrest-core</exclude>
				<exclude>commons-logging*</exclude>
			</excludes>
		</dependencySet>
	</dependencySets>
</assembly>