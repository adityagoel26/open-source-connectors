<assembly>
    <id>car</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <fileSets>
        <fileSet>
            <directory>src/main/resources</directory>
            <outputDirectory>META-INF</outputDirectory>
            <includes>
                <include>connector-config.xml</include>
                <include>com.boomi.Overrides</include>
            </includes>
        </fileSet>
    </fileSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/lib</outputDirectory>
            <excludes>
                <exclude>com.boomi.connsdk:connector-sdk-api</exclude>
                <exclude>com.boomi.commonsdk:common-sdk</exclude>
                <exclude>io.grpc*:*</exclude>
                <exclude>jackson-core*</exclude>
                <exclude>boomi-util*</exclude>
                <exclude>jackson-databind*</exclude>
                <exclude>jackson-annotations*</exclude>
                <exclude>*common*lang*</exclude>
                <exclude>*common*logging*</exclude>
                <exclude>*common*codec*</exclude>
                <exclude>*jsr*</exclude>
                <exclude>*surefire*</exclude>
                <exclude>*FastInfoset*</exclude>
                <exclude>*FastInfoset*</exclude>
            </excludes>
        </dependencySet>
    </dependencySets>
</assembly>