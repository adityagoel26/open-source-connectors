{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "operation",
    "targetDataset",
    "schema"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Bucket_for_table_d67d31f030db0108e1181b8d8529b806"
      ],
      "pattern": "^(.*)$"
    },
    "operation": {
      "$id": "#/properties/operation",
      "type": "object",
      "title": "The Operation Schema",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/operation/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "Operation_Type=Replace"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "targetDataset": {
      "$id": "#/properties/targetDataset",
      "type": "object",
      "title": "The Targetdataset Schema",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/targetDataset/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "d67d31f030db0195df029fcf7f297106"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "schema": {
      "$id": "#/properties/schema",
      "type": "object",
      "title": "The Schema Schema",
      "required": [
        "schemaVersion",
        "parseOptions",
        "fields"
      ],
      "properties": {
        "schemaVersion": {
          "$id": "#/properties/schema/properties/schemaVersion",
          "type": "object",
          "title": "The Schemaversion Schema",
          "required": [
            "id"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/schema/properties/schemaVersion/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "Schema_Version=1.0"
              ],
              "pattern": "^(.*)$"
            }
          }
        },
        "parseOptions": {
          "$id": "#/properties/schema/properties/parseOptions",
          "type": "object",
          "title": "The Parseoptions Schema",
          "required": [
            "charset",
            "fieldsEnclosedBy",
            "fieldsDelimitedBy",
            "headerLinesToIgnore",
            "type"
          ],
          "properties": {
            "charset": {
              "$id": "#/properties/schema/properties/parseOptions/properties/charset",
              "type": "object",
              "title": "The Charset Schema",
              "required": [
                "id"
              ],
              "properties": {
                "id": {
                  "$id": "#/properties/schema/properties/parseOptions/properties/charset/properties/id",
                  "type": "string",
                  "title": "The Id Schema",
                  "default": "",
                  "examples": [
                    "Encoding=UTF-8"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
            "fieldsEnclosedBy": {
              "$id": "#/properties/schema/properties/parseOptions/properties/fieldsEnclosedBy",
              "type": "string",
              "title": "The Fieldsenclosedby Schema",
              "default": "",
              "examples": [
                "\""
              ],
              "pattern": "^(.*)$"
            },
            "fieldsDelimitedBy": {
              "$id": "#/properties/schema/properties/parseOptions/properties/fieldsDelimitedBy",
              "type": "string",
              "title": "The Fieldsdelimitedby Schema",
              "default": "",
              "examples": [
                ","
              ],
              "pattern": "^(.*)$"
            },
            "headerLinesToIgnore": {
              "$id": "#/properties/schema/properties/parseOptions/properties/headerLinesToIgnore",
              "type": "integer",
              "title": "The Headerlinestoignore Schema",
              "default": 0,
              "examples": [
                1
              ]
            },
            "type": {
              "$id": "#/properties/schema/properties/parseOptions/properties/type",
              "type": "object",
              "title": "The Type Schema",
              "required": [
                "id"
              ],
              "properties": {
                "id": {
                  "$id": "#/properties/schema/properties/parseOptions/properties/type/properties/id",
                  "type": "string",
                  "title": "The Id Schema",
                  "default": "",
                  "examples": [
                    "Schema_File_Type=Delimited"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        },
        "fields": {
          "$id": "#/properties/schema/properties/fields",
          "type": "array",
          "title": "The Fields Schema",
          "items": {
            "$id": "#/properties/schema/properties/fields/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "name",
              "description",
              "ordinal",
              "type"
            ],
            "properties": {
              "name": {
                "$id": "#/properties/schema/properties/fields/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "Employee"
                ],
                "pattern": "^(.*)$"
              },
              "description": {
                "$id": "#/properties/schema/properties/fields/items/properties/description",
                "type": "string",
                "title": "The Description Schema",
                "default": "",
                "examples": [
                  "Employee"
                ],
                "pattern": "^(.*)$"
              },
              "ordinal": {
                "$id": "#/properties/schema/properties/fields/items/properties/ordinal",
                "type": "integer",
                "title": "The Ordinal Schema",
                "default": 0,
                "examples": [
                  1
                ]
              },
              "precision": {
                "$id": "#/properties/schema/properties/fields/items/properties/precision",
                "type": "integer",
                "optional": true,
                "title": "The Precision Schema",
                "default": 0,
                "examples": [
                  255
                ]
              },
              "type": {
                "$id": "#/properties/schema/properties/fields/items/properties/type",
                "type": "object",
                "title": "The Type Schema",
                "required": [
                  "id"
                ],
                "properties": {
                  "id": {
                    "$id": "#/properties/schema/properties/fields/items/properties/type/properties/id",
                    "type": "string",
                    "title": "The Id Schema",
                    "default": "",
                    "examples": [
                      "Schema_Field_Type=com.boomi.connector.workdayprism.responses.v2.DescribeTableResponse$Type@6cf10d5c"
                    ],
                    "pattern": "^(.*)$"
                  },
                   "descriptor": {
                    "$id": "#/properties/schema/properties/fields/items/properties/type/properties/descriptor",
                    "type": "string",
                    "optional": true,
                    "title": "The Descriptor Schema",
                    "default": "",
                    "examples": [
                      "Text"
                    ],
                    "pattern": "^(.*)$"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}