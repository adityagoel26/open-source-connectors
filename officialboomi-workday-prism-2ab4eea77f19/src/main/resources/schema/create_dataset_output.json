{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "id",
    "name",
    "createdBy",
    "displayName",
    "fields",
    "createdMoment"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "0a74b5c772a10158fc8c29ef6a274d16"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Dell_Boomi_Table_10"
      ],
      "pattern": "^(.*)$"
    },
    "createdBy": {
      "$id": "#/properties/createdBy",
      "type": "object",
      "title": "The Createdby Schema",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/createdBy/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "4e216435c46510012a46eef729ce4e00"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "title": "The Displayname Schema",
      "default": "",
      "examples": [
        "Table Cretaed on 3rd Feb 2020"
      ],
      "pattern": "^(.*)$"
    },
    "fields": {
      "$id": "#/properties/fields",
      "type": "array",
      "title": "The Fields Schema",
      "items": {
        "$id": "#/properties/fields/items",
        "type": "object",
        "title": "The Items Schema",
        "properties": {
          "id": {
            "$id": "#/properties/fields/items/properties/id",
            "type": "string",
            "optional": true,
            "title": "The Id Schema",
            "default": "",
            "examples": [
              "0a74b5c772a181679ebe2bef6a275616"
            ],
            "pattern": "^(.*)$"
          },
          "description": {
            "$id": "#/properties/fields/items/properties/description",
            "type": "string",
            "optional": true,
            "title": "The Description Schema",
            "default": "",
            "examples": [
              "Employee"
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/fields/items/properties/name",
            "type": "string",
            "optional": true,
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Employee"
            ],
            "pattern": "^(.*)$"
          },
          "displayName": {
            "$id": "#/properties/fields/items/properties/displayName",
            "type": "string",
            "optional": true,
            "title": "The Displayname Schema",
            "default": "",
            "examples": [
              "Employee"
            ],
            "pattern": "^(.*)$"
          },
          "type": {
            "$id": "#/properties/fields/items/properties/type",
            "type": "object",
             "optional": true,
            "title": "The Type Schema",
            "required": [
              "descriptor",
              "id"
            ],
            "properties": {
              "descriptor": {
                "$id": "#/properties/fields/items/properties/type/properties/descriptor",
                "type": "string",
                "title": "The Descriptor Schema",
                "default": "",
                "examples": [
                  "Text"
                ],
                "pattern": "^(.*)$"
              },
              "id": {
                "$id": "#/properties/fields/items/properties/type/properties/id",
                "type": "string",
                "optional": true,
                "title": "The Id Schema",
                "default": "",
                "examples": [
                  "fdd7dd26156610006a12d4fd1ea300ce"
                ],
                "pattern": "^(.*)$"
              }
            }
          },
          "fieldId": {
            "$id": "#/properties/fields/items/properties/fieldId",
            "type": "string",
            "optional": true,
            "title": "The Fieldid Schema",
            "default": "",
            "examples": [
              "OTIwMzg0NWMtMDY0Ny00MjM3LTg4YmYtNWEzNTVhY2MwOWM3"
            ],
            "pattern": "^(.*)$"
          },
          "ordinal": {
            "$id": "#/properties/fields/items/properties/ordinal",
            "type": "integer",
            "optional": true,
            "title": "The Ordinal Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "precision": {
            "$id": "#/properties/fields/items/properties/precision",
            "type": "integer",
            "optional": true,
            "title": "The Precision Schema",
            "default": 0,
            "examples": [
              255
            ]
          }
        }
      }
    },
    "createdMoment": {
      "$id": "#/properties/createdMoment",
      "type": "string",
      "title": "The Createdmoment Schema",
      "default": "",
      "examples": [
        "2020-02-03T12:39:08.526Z"
      ],
      "pattern": "^(.*)$"
    }
  }
}